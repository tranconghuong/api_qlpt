    <script>
      document.onreadystatechange = function () {
        let lastScrollPosition = 0;
        const navbar = document.querySelector(".navbar");
        window.addEventListener("scroll", function (e) {
          lastScrollPosition = window.scrollY;

          if (lastScrollPosition > 400) {
            navbar.classList.add("navbar-dark");
          } else {
            navbar.classList.remove("navbar-dark");
          }
        });
        const bottomTop = document.querySelector(".buttons-bottom");
        window.addEventListener("scroll", function (e) {
          lastScrollPosition = window.scrollY;

          if (lastScrollPosition > 800) {
            bottomTop.classList.add("bottom-to-top");
          } else {
            bottomTop.classList.remove("bottom-to-top");
          }
        });
      };

      function changeLanguage(lang) {
        if (lang == "en") {
          window.location.href = "/";
          return;
        } else window.location.href = "?lang=en";
        return;
      }
    </script>
    <script>
      document.getElementById("logo").addEventListener("click", function () {
        // Scroll to the top of the page
        event.preventDefault();
        window.scrollTo({
          behavior: "smooth", // Add smooth scrolling effect if supported
          top: 0,
        });
      });

      function openMenu() {
        const sideMenu = document.getElementById("side-menu");
        sideMenu.classList.toggle("hidden-menu");
        if (sideMenu.classList.contains("hidden-menu")) {
          sideMenu.style.left = "-250px";
        } else {
          sideMenu.style.left = "0";
        }
        // sideMenu.classList.remove("left-[-250px]");
        // sideMenu.classList.add("left-0");
      }

      function closeMenu() {
        const sideMenu = document.getElementById("side-menu");
        sideMenu.classList.remove("left-0");
        sideMenu.classList.add("left-[-250px]");
      }

      const sidebarItems = document.querySelectorAll(".sidebar-menu a");

      sidebarItems.forEach((item) => {
        const sideMenu = document.getElementById("side-menu");
        item.addEventListener("click", function () {
          sideMenu.classList.toggle("hidden-menu");
          if (sideMenu.classList.contains("hidden-menu")) {
            sideMenu.style.left = "-250px";
          } else {
            sideMenu.style.left = "0";
          }
        });
      });
      function bottomToTop() {
        window.scrollTo({
          behavior: "smooth", // Add smooth scrolling effect if supported
          top: 0,
        });
      }
    </script>
    <script>
