    <style>
      body {
        opacity: 0;
        transition: opacity 0.5s ease-in-out;
      }

      .navbar {
        background-color: #5089fd1a;
        position: absolute;
        top: 0;
        right: 0;
        left: 0;
        width: 100vw;
      }

      .navbar-dark {
        background-color: #def0ea !important;
        position: fixed;
        top: 0;
        right: 0;
        left: 0;
        z-index: 40;
      }

      .button {
        border-radius: 4px;
        background: linear-gradient(to right, #5089fd 50%, #000 50%);
        background-size: 200% 100%;
        background-position: right bottom;
        transition: all 0.5s ease-out;
      }

      .button:hover {
        background-position: left bottom;
      }

      .text {
        text-align: center;
        font-size: 16px;
        line-height: 30px;
        color: black;
        transition: all 0.6s ease-out;
        display: block;
      }

      .text:hover {
        color: white;
      }
      .bottom-to-top {
        position: fixed;
        bottom: 1%;
        right: 2%;
        width: 40px;
        height: 40px;
        border-radius: 4px;
        background-color: #5089fd;
      }
    </style>
